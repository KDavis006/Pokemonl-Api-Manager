<head>
 <link rel="stylesheet" href="../public/Styles/profile.css">
</head>
<body>
<div class="navbar">
 <img class="logo" src="../public/images/blackapi.PNG">
 <a href="/dashboard">Home</a>
 <a href="/pokedex">PokeDex</a>
</div>
<div class="profile-card">
    <img class="profile-picture" src="your_profile_picture_url.jpg" >
    <div class="profile-name"><%= user.username %></div>
</div>
<a href="/users/logout"><button class="absolute-button">Logout</button></a>

<div class="all-main">
    <h1>Favorite Cards</h1>
    <br>
    <div class="card-holder">
        <% user.favorites.forEach(subArray=> { %>
            <div class="sub-array">
                <% subArray.forEach(item=> { %>
                    <div class="item">
                        <img src="<%= item.image %>/high.png" alt="<%= item.name %>" />
                        <h5>
                            <%= item.name %>
                        </h5>
                        <button>s</button>
                    </div>
                    <% }); %>
            </div>
            <% }); %>
    </div>
</div>
</body>